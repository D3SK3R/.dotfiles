_adbe-apps-list ()
{
    local context state state_descr line
    typeset -A opt_args

    _arguments -C \
        ':command:->command' \
        '*::options:->options'

    case $state in
        (command)
            local -a subcommands
            subcommands=(
				'all'
				'system'
				'third-party'
				'debug'
            )
            _values 'adbe apps list' $subcommands
        ;;

        (options)
            case $line[1] in
                all)
                    _adbe-apps-list-all
                ;;
                system)
                    _adbe-apps-list-system
                ;;
                third-party)
                    _adbe-apps-list-third-party
                ;;
                debug)
                    _adbe-apps-list-debug
                ;;
            esac
        ;;
    esac

}

