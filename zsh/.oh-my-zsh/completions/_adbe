                ;;
            esac
        ;;
    esac

}

_adbe-battery-level ()
{
    local context state state_descr line
    typeset -A opt_args

    if [[ $words[$CURRENT] == -* ]] ; then
        _arguments -C \
        ':command:->command' \

    else
        myargs=('<percentage>')
        _message_next_arg
    fi
}

