_adbe-standby-bucket ()
{
    local context state state_descr line
    typeset -A opt_args

    _arguments -C \
        ':command:->command' \
        '*::options:->options'

    case $state in
        (command)
            local -a subcommands
            subcommands=(
				'get'
				'set'
            )
            _values 'adbe standby bucket' $subcommands
        ;;

        (options)
            case $line[1] in
                get)
                    _adbe-standby-bucket-get
                ;;
                set)
                    _adbe-standby-bucket-set
                ;;
            esac
        ;;
    esac

}

_adbe-standby-bucket-get ()
{
    local context state state_descr line
    typeset -A opt_args

    if [[ $words[$CURRENT] == -* ]] ; then
        _arguments -C \
        ':command:->command' \

    else
        myargs=('<app_name>')
        _message_next_arg
    fi
}

_adbe-standby-bucket-set ()
{
    local context state state_descr line
    typeset -A opt_args

    if [[ $words[$CURRENT] == -* ]] ; then
        _arguments -C \
        ':command:->command' \

    else
